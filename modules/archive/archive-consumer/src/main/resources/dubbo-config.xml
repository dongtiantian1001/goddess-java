<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context-3.0.xsd
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://code.alibabatech.com/schema/dubbo 
		  http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <context:property-placeholder location="classpath:dubbo.properties"/>

    <dubbo:application name="${dubbo.application.name}"/>
    <dubbo:consumer check="false" timeout="15000"/>
    <dubbo:registry address="${dubbo.registry.address}"/>

    <!--以下配置为注册业务接口,所有控制器,业务层等用到的业务接口都需在此配置-->
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ArchiveAccessAPI"
                     id="archiveAccessApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ArchiveDetailAPI"
                     id="archiveDetailApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ResumeInfoAPI"
                     id="resumeInfoApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ArchiveGatherAPI"
                     id="archiveGatherApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.EnclosureTypeAPI"
                     id="enclosureTypeApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ForeignStaffingAPI"
                     id="foreignStaffingApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.ForeignStaffingSetAPI"
                     id="foreignStaffingSetApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.LaborRelationAPI"
                     id="laborRelationApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.PersonnelQualificationAPI"
                     id="personnelQualificationApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.SocialSecurityTypeAPI"
                     id="socialSecurityTypeApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.StaffRecordsAPI"
                     id="staffRecordsApiImpl"/>

    <dubbo:reference validation="true" interface="com.bjike.goddess.storage.api.FileAPI" id="fileApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.storage.api.StorageUserAPI"
                     id="storageUserApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.user.api.UserAPI" id="userApiImpl"/>

    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.RotainCusPermissionAPI"
                     id="rotainCusPermissionApiImpl"/>
    <dubbo:reference validation="true" interface="com.bjike.goddess.archive.api.RotainPermissionOperateAPI"
                     id="rotainPermissionOperateApiImpl"/>

    <dubbo:reference validation="true" interface="com.bjike.goddess.user.api.rbac.PermissionAPI"
                     id="permissionApiImpl"/>

    <dubbo:reference validation="true" id="departmentDetailApiImpl"
                     interface="com.bjike.goddess.organize.api.DepartmentDetailAPI"/>
    <dubbo:reference validation="true" id="arrangementApiImpl"
                     interface="com.bjike.goddess.organize.api.ArrangementAPI"/>
    <dubbo:reference validation="true" id="moduleTypeApiImpl" interface="com.bjike.goddess.organize.api.ModuleTypeAPI"/>
    <dubbo:reference validation="true" id="positionDetailApiImpl"
                     interface="com.bjike.goddess.organize.api.PositionDetailAPI"/>
    <dubbo:reference validation="true" id="positionDetailUserApiImpl"
                     interface="com.bjike.goddess.organize.api.PositionDetailUserAPI"/>
    <dubbo:reference validation="true" id="userSetPermissionApiImpl"
                     interface="com.bjike.goddess.organize.api.UserSetPermissionAPI"/>
    <dubbo:reference validation="true" id="moduleApiImpl" interface="com.bjike.goddess.assemble.api.ModuleAPI"/>
    <dubbo:reference validation="true" id="entryRegisterApiImpl" interface="com.bjike.goddess.staffentry.api.EntryRegisterAPI"/>
    <dubbo:reference validation="true" id="qualificationsCollectApiImpl" interface="com.bjike.goddess.qualifications.api.QualificationsCollectAPI"/>
    <dubbo:reference validation="true" id="employeeSecureApiImpl" interface="com.bjike.goddess.secure.api.EmployeeSecureAPI"/>

</beans>
