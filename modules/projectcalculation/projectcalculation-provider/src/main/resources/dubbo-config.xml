<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context-3.0.xsd
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://code.alibabatech.com/schema/dubbo 
		  http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <context:property-placeholder location="classpath:dubbo.properties"/>

    <dubbo:application name="${dubbo.application.name}"/>
    <dubbo:provider timeout="${dubbo.provider.timeout}" filter="userFilter"/>
    <dubbo:consumer check="false" timeout="15000"/>
    <dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}"/>
    <dubbo:registry address="${dubbo.registry.address}"/>

    <!--发布接口,供其他模块调用-->
    <!--另外注意命名,UserAPI为例,UserAPI是业务接口,则userSer是实现类@Service("userSer") 里注解的名字-->
    <dubbo:service interface="com.bjike.goddess.projectcalculation.api.CalculationDetailAPI" ref="calculationDetailApiImpl"/>
    <dubbo:service interface="com.bjike.goddess.projectcalculation.api.CalculationCollectAPI" ref="calculationCollectApiImpl"/>
    <dubbo:service interface="com.bjike.goddess.projectcalculation.api.CusPermissionAPI" ref="cusPermissionApiImpl"/>
    <dubbo:service interface="com.bjike.goddess.projectcalculation.api.CalculationDecisionsAPI" ref="calculationDecisionsApiImpl"/>
    <dubbo:service interface="com.bjike.goddess.projectcalculation.service.CusPermissionSer" ref="cusPermissionSerImpl"/>
    <dubbo:service interface="com.bjike.goddess.projectcalculation.api.InterfaceCalculationDetailAPI" ref="interfaceCalculationDetailApiImpl"/>
    <!--<dubbo:service interface="com.bjike.goddess.projectcalculation.api.CalculationDecisionsAPI" ref="calculationDetailApiImpl"/>-->
    <!--<dubbo:service interface="com.bjike.goddess.projectcost.api.CarCostAPI" ref="carCostApiImpl"/>-->
    <!--<dubbo:service interface="com.bjike.goddess.projectcost.api.CostControlAPI" ref="costControlApiImpl"/>-->
    <!--<dubbo:service interface="com.bjike.goddess.projectcost.api.OtherExpensesAPI" ref="projectOtherExpensesApiImpl"/>-->


    <!--<dubbo:service  ref="cusPermissionApiImpl" interface="com.bjike.goddess.projectcost.api.CusPermissionAPI"></dubbo:service>-->
    <!--<dubbo:service  ref="cusPermissionOperateApiImpl" interface="com.bjike.goddess.projectcost.api.CusPermissionOperateAPI" />-->

    <!--&lt;!&ndash;以下配置为(上面的依赖注入接口所依赖的),如上面的 UserAPI的某个业务注入了UserLoginAPI 册需要配置该关系.-->
    <!--否则不需要配置&ndash;&gt;-->
    <!--<dubbo:reference validation="true" interface="com.bjike.goddess.otherexpenses.api.OtherExpensesAPI"-->
                     <!--id="otherExpensesApiImpl"/>-->
    <!--<dubbo:reference  validation="true" interface="com.bjike.goddess.dispatchcar.api.DispatchCarInfoAPI" id="dispatchCarInfoApiImpl" />-->
    <dubbo:reference  validation="true" interface="com.bjike.goddess.user.api.UserAPI" id="userApiImpl" />
    <!--<dubbo:reference  validation="true" interface="com.bjike.goddess.projectcalculation.service.CusPermissionSer" id="cusPermissionSerImpl" />-->
    <dubbo:reference validation="true" id="arrangementApiImpl"
                     interface="com.bjike.goddess.organize.api.ArrangementAPI"/>
    <dubbo:reference validation="true" id="departmentDetailApiImpl"
                     interface="com.bjike.goddess.organize.api.DepartmentDetailAPI"/>
    <dubbo:reference validation="true" id="moduleTypeApiImpl" interface="com.bjike.goddess.organize.api.ModuleTypeAPI"/>
    <dubbo:reference validation="true" id="positionDetailApiImpl"
                     interface="com.bjike.goddess.organize.api.PositionDetailAPI"/>
    <!--<dubbo:reference validation="true" id="positionDetailUserApiImpl"-->
    <dubbo:reference validation="true" id="marketInfoPreAnalysisApiImpl"
                     interface="com.bjike.goddess.market.api.MarketInfoPreAnalysisAPI"/>
    <dubbo:reference validation="true" id="messageApiImpl"
                     interface="com.bjike.goddess.message.api.MessageAPI"/>
    <dubbo:reference validation="true" id="cusPermissionOperateSerImpl"
                     interface="com.bjike.goddess.projectcalculation.service.CusPermissionOperateSer"/>


</beans>
